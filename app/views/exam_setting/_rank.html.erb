<div class="row">
  <div class="col-md-12">
    <div class="pull-right">
      <%= link_to 'Create Rank',settingrank_exam_setting_path(@course),remote:true , class: "btn btn-primary-link btn-xs" %>
    </div>
  </div>
</div>
<div class="row">
  <div class="box col-md-12">
    <div class="box-inner">
      <div class="box-header well">
        <h2>Ranking Level Details</h2>
      </div>
      <div class="box-content">
        <table id="datatable" class="table table-striped table-bordered responsive">
          <thead>
            <tr>
              <th>Ranking Levels</th>
              <th></th>
              <th></th>
              <th>Action</th>
            </tr>
          </thead>
          <% if @rank_levels.empty? %>
            <tr>
              <th colspan="4"><center>No record found.</center></th>
            </tr>
          <%else%>  
            <tbody class="product-index">
              <%= render 'newrank' %>
            </tbody>
          <%end%>  
        </table>
      </div>
    </div>
  </div>
</div>
