 $("ul.errors").html("")
<% if   @rank_lev1.errors.any? %>
		<%   @rank_lev1.errors.full_messages.each do |message| %>
		$("div.errors").addClass("alert alert-danger alert-dismissable")  
		$("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
		<% end %>
<% else %>
$('#rank_div').html("<%= j render 'rank'%>");
$('#flash').html("<%= j render 'layouts/messages'%>")
  $("#myModalrank").modal("hide")
 <%end%>