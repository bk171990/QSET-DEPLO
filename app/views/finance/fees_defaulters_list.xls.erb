   <table cellpadding="5" cellspacing="5" width="700">
      <% unless @students.nil?%>    
        <tr bgcolor="99CCFF">
          <th>Sl no</th>
          <th>Admission no</th>
          <th>Student Name</th>
        </tr>
        <% @students.each_with_index do |s,i|%>
          <%@fee=FinanceFee.find_by_student_id_and_finance_fee_collection_id(s.id,@collection.id)%>
          <% unless @fee.is_paid? %>
            <tr>
              <td><%= i+1 %></td>
              <td><%= s.admission_no%></td>
              <td><%= s.first_name+" "+s.last_name %></td>
            </tr>
          <%end%>
        <%end%>
      <%end%>
      </table>