<div id="content-header">
  <h1>Exam Reports</h1>
  <h3>Ranking Level Report</h3>
  <div id="app-back-button">
    <%= link_to image_tag("buttons/back.png",:border => 0),:back  %>
  </div>
<div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>
<div id="inner-tab-menu">
  <ul>
    <li><%= link_to "Pdf Report"%></li>
  </ul>
</div>

<div id="page-yield">
  <div class="box">
      <h4><%= @ranking_level.name %> Students : <%= @batch.course.course_name+" "+@batch.course.section_name+" "+@batch.name %> <%= " | #{Subject} : "+@subject.name if @subject.present? %></h4>
      <div id="score-table">
            <table id="listing" width="100%" cellpadding="1" cellspacing="1">
              <tr class="tr-head">
                <td><%= t('sl_no') %></td>
                <td><%= t('adm_no') %></td>
                <td><%= t('name') %></td>
              </tr>
              <tr class="tr-blank"></tr>
              <% @students.each_with_index do |s,i| %>
                <tr class="tr-<%= cycle('odd', 'even') %>">
                  <td class="col-1"><%= i+1 %></td>
                  <td class="col-1"><%= s.admission_no.present? ? s.admission_no : "-" %></td>
                  <td class="col-2"><%= s.first_name+" "+s.last_name%></td>
                </tr>
              <% end %>
            </table>
  </div>
</div>
</div>
