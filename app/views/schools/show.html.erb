<div>
  <ul class="breadcrumb">
    <li><%= link_to 'Home',dashboard_home_index_path %></li>
    <li><%= link_to 'School',schools_path %></li>
  </ul>
</div>
<div id="flash"><%= render 'layouts/messages'%></div>
<div class="row">
  <div class="col-md-12">
    <div class="pull-right">
    </div>
  </div>
</div>
<hr>
<div class="row">
  <div class="box col-md-12">
  <div class="box-inner">
    <div class="box-header well">
      <h2>School User Details</h2>
    </div>

    <div class="box-content">
      <table id="datatable" class="table table-striped table-bordered responsive">
        <thead>
          <tr>
            <th>School Name</th>
            <th>UserName</th>
            <th>Password</th>
            <th>Contact</th>
            <th> Email</th>
            <th>Address</th>
            <th>Organization</th>
          </tr>
        </thead>
        <tbody>
       
          <tr>
            
            <td><%= @school.name %></td>
            <td><%= @school.username %></td>
            <td><%= @school.password%></td>
            <td><%= @school.contact %></td>
            <td><%= @school.email %></td>
            <td><%= @school.address %></td>
            <td><%= @school.organization %></td>

           
          </tr>
         
        </tbody>
      </table>
    </div>

  </div>
  </div>
</div>